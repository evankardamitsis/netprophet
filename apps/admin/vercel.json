{
  "buildCommand": "cd ../.. && pnpm run build --filter=@netprophet/admin",
  "outputDirectory": ".next",
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "ignoreCommand": "cd ../.. && npx turbo-ignore @netprophet/admin --fallback=HEAD^",
  "crons": [
    {
      "path": "/api/cron/process-emails",
      "schedule": "* * * * *"
    }
  ]
}
